{
  "name": "Workout-APP",
  "version": "1.0.0",
  "description": "Workout App",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"node server\"",
    "server": "nodemon server.js",
    "client-workout": "npm start --prefix client-workout",
    "client-workoutinstall": "npm install --prexfix client-workout",
    "dev": "concurrently \"npm run server\" \"npm run client-workout\"",
    "clean": "rm -rf node_modules",
    "build": "cd client-workout && npm run clean && npm install --only=dev && npm install && npm run-script build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.4",
    "axios": "^0.19.2",
    "uuid": "^7.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-transition-group": "^4.3.0",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
